{
	"$schema": "https://swc.rs/schema.json",
	"sourceMaps": false,
	"jsc": {
		"baseUrl": "./",
		"paths": {
			"src/*": ["src/*"]
		},
		"parser": {
			"syntax": "typescript",
			"decorators": true,
			"dynamicImport": true
		},
		"transform": {
			"legacyDecorator": true,
			"decoratorMetadata": true
		}
	}
}
